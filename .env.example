# Environment variables for AI/ML Testing Framework

# API Configuration
security-platform_API_TOKEN=your_api_token_here
security-platform_API_BASE_URL=https://api.security-platform.ai

# AWS Configuration (if needed)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_DEFAULT_REGION=us-east-1

# Model Configuration
MODEL_VERSION=1.0.0
EMBEDDINGS_MODEL=all-MiniLM-L6-v2

# Testing Configuration
PARALLEL_WORKERS=4
TEST_TIMEOUT=30
LOG_LEVEL=INFO

# Monitoring & Reporting
ENABLE_METRICS=true
METRICS_ENDPOINT=https://metrics.security-platform.ai
